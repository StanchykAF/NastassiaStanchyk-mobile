<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Web mobile testing Suite">
    <parameter name="appType" value="web"/>
    <parameter name="deviceName" value="Android GoogleAPI Emulator"/>
    <parameter name="deviceOrientation" value="portrait"/>
    <parameter name="platformName" value="Android"/>
    <parameter name="platformVersion" value="13.0"/>
    <parameter name="browserName" value="Chrome"/>
    <parameter name="automationName" value="UiAutomator2"/>
    <parameter name="appiumVersion" value="1.22.3"/>
    <test name="Web tests">
        <groups>
            <run>
                <include name="web"/>
            </run>
        </groups>
        <packages>
            <package name="hw3.scenarios"/>
        </packages>
    </test>
</suite>