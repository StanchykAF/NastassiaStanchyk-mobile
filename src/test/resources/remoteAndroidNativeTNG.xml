<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Native mobile testing Suite">
    <parameter name="appType" value="native"/>
    <parameter name="deviceName" value="Android GoogleAPI Emulator"/>
    <parameter name="platformName" value="Android"/>
    <parameter name="platformVersion" value="13.0"/>
    <parameter name="app" value="EPAMTestApp.apk"/>
    <parameter name="automationName" value="UiAutomator2"/>
    <parameter name="appiumVersion" value="2.0.0"/>
    <test name="Native tests">
        <groups>
            <run>
                <include name="native"/>
            </run>
        </groups>
        <packages>
            <package name="hw3.scenarios"/>
        </packages>
    </test>
</suite>