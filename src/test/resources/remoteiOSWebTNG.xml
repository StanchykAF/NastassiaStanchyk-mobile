<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Web mobile testing Suite">
    <parameter name="appType" value="web"/>
    <parameter name="deviceName" value="iPhone Simulator"/>
    <parameter name="deviceOrientation" value="portrait"/>
    <parameter name="platformName" value="iOS"/>
    <parameter name="platformVersion" value="16.2"/>
    <parameter name="browserName" value="Safari"/>
    <parameter name="automationName" value="XCUITest"/>
    <parameter name="appiumVersion" value="2.0.0"/>
    <test name="Web tests">
        <groups>
            <run>
                <include name="web"/>
            </run>
        </groups>
        <packages>
            <package name="hw3.scenarios"/>
        </packages>
    </test>
</suite>